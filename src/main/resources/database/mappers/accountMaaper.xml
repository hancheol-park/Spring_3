<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.abc.s3.account.AccountDAO">
    
    	<select id="getList" parameterType="AccountDTO">
		select * from account where id=#{id}
		</select>
		
		<insert id="setInsert" parameterType="AccountDTO">
			insert into account values(#{accountNumber}, #{accoountBalance},sysdate,#{id},#{bookNumber})
		</insert>
    
    </mapper>